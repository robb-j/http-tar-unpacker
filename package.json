{
  "name": "@openlab/http-tar-unpacker",
  "description": "Unpack a tar from a http request",
  "version": "0.1.0",
  "private": true,
  "repository": "openlab/http-tar-unpacker",
  "author": "",
  "license": "MIT",
  "scripts": {
    "coverage": "jest --coverage",
    "dev": "NODE_ENV=development nodemon -w src -x 'node -r dotenv/config' src/index.js",
    "lint": "eslint src",
    "prettier": "prettier --write '**/*.{js,json,css,md,ts,tsx}'",
    "preversion": "npm run test -s",
    "postversion": "bin/release",
    "start": "node -r dotenv/config src/index.js",
    "test": "jest"
  },
  "keywords": [],
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs-extra": "^9.0.0",
    "tar": "^6.0.1",
    "valid-env": "^1.0.1"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "lint-staged": "^10.1.3",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.4"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "main": "src/index.js"
}
